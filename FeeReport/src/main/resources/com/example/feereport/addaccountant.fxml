         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Label layoutX="61.0" layoutY="246.0" text="Username" textFill="#0062ff">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Label layoutX="59.0" layoutY="318.0" text="Password" textFill="#0062ff">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <TextField fx:id="nameField" layoutX="94.0" layoutY="130.0" prefHeight="25.0" prefWidth="213.0" promptText="name" />
